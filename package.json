{
  "name": "cannabis.ca.gov",
  "version": "1.0.0",
  "description": "Headless build of cannabis.ca.gov",
  "main": "index.html",
  "scripts": {
    "sass:compile": "node-sass --source-map true src/css/sass/ -o dist/",
    "sass:build": "npm run sass:compile && npm run build:regen",
    "sass:watch": "chokidar 'src/**/*.scss' -c 'npm run sass:build'",
    "build:regen": "npx @11ty/eleventy --quiet",
    "build:watch": "npx @11ty/eleventy --quiet --watch",
    "js:watch": "chokidar 'src/**/*.js' -c 'npm run js:bundle && npm run build:regen'",
    "js:bundle": "rollup --config rollup.config.js",
    "start": "npx @11ty/eleventy --quiet --serve",
    "dev": "npm-run-all -p sass:build js:bundle sass:watch js:watch build:watch start",
    "build": "npm-run-all -s js:bundle sass:build build:regen"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cagov/cannabis.ca.gov/issues"
  },
  "homepage": "https://github.com/cagov/cannabis.ca.gov#README",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "node-fetch": "^2.6.1"
  },
  "dependencies": {
    "@cagov/11ty-serverless-preview-mode": "^1.0.6",
    "@cagov/ds-accordion": "^1.0.3",
    "@cagov/ds-feedback": "^1.0.5",
    "@cagov/ds-minus": "^1.0.0",
    "@cagov/ds-pagination": "^1.0.0",
    "@cagov/ds-plus": "^1.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.2.5",
    "chokidar-cli": "^2.1.0",
    "clean-css": "^5.1.2",
    "moment-timezone": "^0.5.33",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
