{
  "name": "cannabis.ca.gov",
  "version": "2.0.0",
  "description": "Static site generator for https://headless.cannabis.ca.gov",
  "main": "index.html",
  "scripts": {
    "build": "eleventy",
    "dev": "npm run site:watch:dev",
    "site:build:dev": "npm run content:update && cross-env NODE_ENV=development SITE_ENV=staging eleventy",
    "site:build:staging": "npm run content:update:staging && cross-env SITE_ENV=staging eleventy",
    "site:watch:dev": "cross-env NODE_ENV=development eleventy --watch --serve --incremental",
    "site:watch:staging": "cross-env NODE_ENV=development SITE_ENV=staging eleventy --watch --serve --incremental",
    "site:watch": "eleventy --serve",
    "start": "npm run site:watch",
    "deploy:redirects": "node src/deploy/redirects/assign.js",
    "test:setup": "npx playwright install-deps chromium && npx playwright install",
    "test:serve": "npx http-server --port 8080 docs/",
    "test": "npx playwright test test/e2e.spec.mjs --project=chromium --config test/playwright.e2e.config.js",
    "linkcheck:dev": "npx playwright test test/linkcheck.spec.mjs --config=test/playwright-dev.config.js",
    "linkcheck:gh": "npx playwright test test/linkcheck.spec.mjs --config=test/playwright.config.js",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "code:format": "npm run format:prettier --ignore-path",
    "content:clean": "rm -rf docs && rm -rf pages/_content/menu && rm -rf pages/_content/pages && rm -rf pages/_content/posts && rm -rf pages/_content/redirects && npm run build",
    "content:update": "npm update static-content-cannabis",
    "content:update:staging": "npm update static-content-cannabis-staging"
  },
  "keywords": [
    "@cagov",
    "Office of Data and Innovation",
    "Department of Cannabis Control"
  ],
  "repository": "https://github.com/cannabis.ca.gov",
  "author": "Office of Data and Innovation & Department of Cannabis Control",
  "license": "MIT",
  "devDependencies": {
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^3.1.6",
    "@playwright/test": "^1.25.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@web/dev-server": "^0.1.17",
    "aws-sdk": "^2.1200.0",
    "axe-playwright": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "^5.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "html-minifier": "^4.0.0",
    "prettier": "^2.4.1",
    "rollup-plugin-terser": "^7.0.2",
    "static-content-cannabis-debugging": "github:cagov/static-content-cannabis#main-reduced-content",
    "static-content-cannabis-staging": "github:cagov/static-content-cannabis#staging"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-canary.15",
    "@11ty/eleventy-server-browsersync": "^1.0.2",
    "@cagov/11ty-build-system": "^1.0.0-beta.2",
    "@cagov/ds-accordion": "^2.0.3",
    "@cagov/ds-agency-footer": "^1.0.2",
    "@cagov/ds-back-to-top": "^2.0.2",
    "@cagov/ds-base-css": "^2.0.0-beta.1",
    "@cagov/ds-branding": "^1.0.8",
    "@cagov/ds-button-grid": "^2.0.2",
    "@cagov/ds-dropdown-menu": "^2.0.1",
    "@cagov/ds-feature-card": "^2.0.0",
    "@cagov/ds-google-translate": "^1.0.2",
    "@cagov/ds-icons": "0.0.1",
    "@cagov/ds-link-grid": "^3.0.0",
    "@cagov/ds-link-icon": "^2.0.0",
    "@cagov/ds-page-alert": "^2.0.2",
    "@cagov/ds-page-feedback": "^1.1.3",
    "@cagov/ds-page-navigation": "^2.2.0",
    "@cagov/ds-pagination": "^2.0.2",
    "@cagov/ds-regulatory-outline": "^1.0.1",
    "@cagov/ds-site-footer": "^2.0.0",
    "@cagov/ds-site-header": "^2.0.0",
    "@cagov/ds-site-navigation": "^3.0.0",
    "@cagov/ds-skip-to-content": "^2.0.0",
    "@cagov/ds-statewide-footer": "^2.0.0",
    "@cagov/ds-statewide-header": "^2.0.0",
    "@cagov/ds-step-list": "^3.0.0",
    "@cagov/ds-table": "^1.0.1",
    "static-content-cannabis": "github:cagov/static-content-cannabis#main"
  }
}
